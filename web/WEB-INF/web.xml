<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
         http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
    <!--审核完后的人员名单表格分页处理-->
    <servlet>
        <servlet-name>Examine_paging</servlet-name>
        <servlet-class>Paging.Examine_paging</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Examine_paging</servlet-name>
        <url-pattern>/paging</url-pattern>
    </servlet-mapping>

    <!--待审和的人员名单处理-->
    <servlet>
        <servlet-name>Apply_paging</servlet-name>
        <servlet-class>Paging.Apply_paging</servlet-class>
     </servlet>
    <servlet-mapping>
        <servlet-name>Apply_paging</servlet-name>
        <url-pattern>/apply_paging</url-pattern>
    </servlet-mapping>
    <!--教室管理员删除审核后的记录-->
    <servlet>
        <servlet-name>Manage</servlet-name>
        <servlet-class>deleteNotes.ManageDelete</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Manage</servlet-name>
        <url-pattern>/grateDelete</url-pattern>
    </servlet-mapping>
    <!--审核老师删除过期无效数据-->
    <servlet>
        <servlet-name>Examine</servlet-name>
        <servlet-class>deleteNotes.ExamineDelete</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Examine</servlet-name>
        <url-pattern>/examineDelete</url-pattern>
    </servlet-mapping>
    <!--审核老师审核数据-->
    <servlet>
        <servlet-name>Examineadd</servlet-name>
        <servlet-class>deleteNotes.ExamineAdd</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Examineadd</servlet-name>
        <url-pattern>/examinAdd</url-pattern>
    </servlet-mapping>

    <!--验证码图片的生成 src-->
    <servlet>
        <servlet-name>DrawImage</servlet-name>
        <servlet-class>verification.DrawImage</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DrawImage</servlet-name>
        <url-pattern>/DrawImage</url-pattern>
    </servlet-mapping>
    <!--审核老师用户删除-->
    <servlet>
        <servlet-name>threedelete</servlet-name>
        <servlet-class>deleteNotes.ThreeDelete</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>threedelete</servlet-name>
        <url-pattern>/manageUser/threedelect</url-pattern>
    </servlet-mapping>

    <!--审核老师用户列表-->
    <servlet>
        <servlet-name>Three</servlet-name>
        <servlet-class>Paging.Three_paging</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Three</servlet-name>
        <url-pattern>/manageUser/three_paging</url-pattern>
    </servlet-mapping>
</web-app>